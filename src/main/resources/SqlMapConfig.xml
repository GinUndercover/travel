<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
<!-- 延迟加载配置 -->
<settings>
<!-- 开启延迟加载为true -->
  <setting name="lazyLoadingEnabled" value="true"/>
  <!-- 设置积极的懒加载 -->
   <setting name="aggressiveLazyLoading" value="false"/>
</settings>

<!-- 自定义别名 -->
<typeAliases>
   <!-- 单个别名定义 -->
<!--    <typeAlias type="com.aishang.domain.User" alias="user"/> -->
   
   <!-- 批量别名定义(推荐) -->
   <!-- package：指定包名称，来为该包下所有po类声明别名，默认的别名就是类名（首字母大小写都可以）-->
   <package name="com.baoxun.po"/>
</typeAliases>
</configuration>